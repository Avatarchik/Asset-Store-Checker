{
  "name": "Asset Store Checker",
  "description": "Gets information about your sales on the Asset Store.",
  "version": "1.0",
  "icons": { "128": "icon.128.png" },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
	"https://publisher.assetstore.unity3d.com/",
	"alarms"
